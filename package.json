    "webpack": "^3.11.0",
    "yup": "0.26.10"
  },
  "scripts": {
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && env-cmd .env.${BUILD_ENV} react-scripts-ts build",
    "build:codebnb": "BUILD_ENV=codebnb npm run build",
    "build:production": "BUILD_ENV=production npm run build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/assets/scss/main.scss -o src/assets/scss/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss/ -o src/assets/scss/ --watch --recursive"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.1",
    "@types/normalizr": "^2.0.18",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.6",
    "redux-devtools-extension": "^2.13.7",
    "typescript": "^3.1.2",
    "env-cmd": "^8.0.2"
  }
}
