    "core-js": "^2.6.4",
    "formik": "^1.3.0",
    "humps": "^2.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.23.0",
    "mouse-wheel-zoom": "^1.1.4",
    "node-sass-chokidar": "^1.3.3",
    "normalizr": "^3.2.4",
    "npm": "^6.9.0",
    "npm-run-all": "^4.1.3",
    "pdfjs-dist": "^2.0.943",
    "react": "^16.5.2",
    "react-dates": "^18.0.4",
    "react-dom": "^16.5.2",
    "react-dropzone": "^7.0.1",
    "react-easy-crop": "^1.7.0",
    "react-image-magnify": "^2.7.4",
    "react-image-zoom": "^0.7.0",
    "react-paginate": "^6.0.0",
    "react-redux": "^5.0.7",
    "react-responsive-modal": "^3.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "react-select": "2.3.0",
    "react-slick": "^0.23.2",
    "react-slideshow-image": "^1.2.0",
    "redux": "^4.0.0",
